num1 = int(input("Enter first number:"))
num2 = int(input("Enter second number:"))

num = int(input("please enter number between 1 - 3"))

match num:
    case 1:
        num = num1+num2
        print("Addition",num)
    case 2:
        num = num1-num2
        print("Subtraction",num)
    case 3:
        num = num1*num2
        print("Mutiple",num)
    case _:
        print("InVaild input")

